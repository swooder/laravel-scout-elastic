{
    "name": "woodfish/laravel-scout-elastic",
    "description": "Elastic Driver for Laravel Scout",
    "authors": [
        {
            "name": "shaojie",
            "email": "shaojie519@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "laravel/scout": "^3.0",
        "elasticsearch/elasticsearch": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Woodfish\\Elasticsearch\\": "src/"
        }
    },
    "suggest": {
        "elasticsearch/elasticsearch": "Required to use the Elasticsearch engine (^5.0)."
    },
    "extra": {
        "laravel": {
            "providers": [
                "Woodfish\\Elasticsearch\\ElasticsearchProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true


}
